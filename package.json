{
  "name": "@wireside/dashboard-api",
  "version": "1.0.0",
  "description": "API для учебного проекта",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec 'tsc --noEmit && tsx --inspect=localhost:9229 ./src/main.ts'",
    "lint": "eslint \"./src/**/*.{js,ts,tsx,jsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,ts,tsx,jsx}\"",
    "build": "tsc",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "author": "Bogdan Salaev",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "inversify": "^7.5.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "reflect-metadata": "^0.2.2",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.8.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
